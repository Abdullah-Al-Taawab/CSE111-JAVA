import java.util.Scanner;
public class task03
{
  public static void main(String [] args)
  {
    Scanner sc=new Scanner(System.in);
    int a[]=new int[5];
    int temp;
    for(int c=0;c<a.length;c++){
      System.out.println("Enter a num");
      a[c]=sc.nextInt();
    }
    for(int c=0;c<a.length;c++){
      for( int i=0;i<a.length;i++){
        if (a[c]>a[i]){
          temp=a[c];
          a[c]=a[i];
          a[i]=temp;
        }
      }
    }
        for(int c=0;c<a.length;c++){
          System.out.print(a[c]);
        }
      }
}
      