public class Cat{ 
public String color = "White"; 
public String action = "sitting"; 
public Cat(){
}
public Cat(String n){
  color=n;
}
public Cat(String c, String a){
  color=c;
  action=a;
}
public void printCat(){
  System.out.println(color + "cat is" +action);
}
public void changeColor(String n){
  color=n;
}
} 
